		"typography": {
			"dropCap": false,
			"fluid": true,
			"textDecoration": false,
			"fontFamilies": [
				{
					"fontFamily": "\"Serenity\", sans-serif",
					"name": "Serenity",
					"slug": "serenity",
					"fontFace": [
						{
							"fontFamily": "Serenity",
							"fontWeight": "300",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./fonts/serenity-light-webfont.woff2" ]
						},
						{
							"fontFamily": "Serenity",
							"fontWeight": "300",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [ "file:./fonts/serenity-lightitalic-webfont.woff2" ]
						},
						{
							"fontFamily": "Serenity",
							"fontWeight": "600",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./fonts/serenity-bold-webfont.woff2" ]
						},
