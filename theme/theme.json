			"contentSize": "60rem",
			"wideSize": "80rem"
		},
		"spacing": {
			"blockGap": true
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "8px",
					"color": "transparent",
					"width": "2px"
				},
				"color": {
					"background": "var(--wp--preset--color--aqua-800)",
					"text": "var(--wp--preset--color--aqua-50)"
				},
				"variations": {
					"outline": {
						"border": {
							"color": "var(--wp--preset--color--aqua-800)",
							"style": "solid",
							"width": "2px"
						},
						"color": {
							"background": "transparent",
							"text": "var(--wp--preset--color--aqua-800)"
						}
					}
				}
			},
			"core/post-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serenity)",
					"fontWeight": "300",
					"fontSize": "var(--wp--preset--font-size--3-xl)"
				}
			},
			"core/pullquote": {
				"border": {
					"width": "1px 0"
				}
			},
			"core/query-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serenity)",
					"fontWeight": "300",
					"fontSize": "var(--wp--preset--font-size--2-xl)"
				}
			},
			"core/quote": {
				"border": {
					"width": "1px"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serenity)",
					"fontSize": "var(--wp--preset--font-size--4-xl)",
					"fontWeight": "normal"
				}
			}
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serenity)",
					"fontWeight": "300",
					"fontSize": "var(--wp--preset--font-size--4-xl)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serenity)",
					"fontWeight": "300",
					"fontSize": "var(--wp--preset--font-size--3-xl)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serenity)",
					"fontWeight": "300",
					"fontSize": "var(--wp--preset--font-size--2-xl)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serenity)",
					"fontWeight": "300",
					"fontSize": "var(--wp--preset--font-size--xl)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serenity)",
					"fontWeight": "300",
					"fontSize": "var(--wp--preset--font-size--lg)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serenity)",
					"fontWeight": "300",
					"textTransform": "uppercase",
					"fontSize": "var(--wp--preset--font-size--base)"
				}
			},
			"button": {
				"border": {
					"radius": ".5rem",
					"color": "transparent"
				},
				"color": {
					"background": "var(--wp--preset--color--aqua-700)",
					"text": "var(--wp--preset--color--aqua-50)"
				},
				"spacing": {
					"padding": {
						"bottom": "0.6rem",
						"left": "1rem",
						"right": "1rem",
						"top": "0.6rem"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serenity)",
					"fontSize": "var(--wp--preset--font-size--base)",
					"fontStyle": "normal",
					"fontWeight": "600",
					"textDecoration": "none"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--aqua-600)",
						"text": "var(--wp--preset--color--white)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--contrast-2)",
						"text": "var(--wp--preset--color--white)"
					},
					"outline": {
						"color": "var(--wp--preset--color--contrast)",
						"offset": "2px",
						"style": "dotted",
						"width": "1px"
					},
					"border": {
						"color": "var(--wp--preset--color--contrast-2)"
					}
				},
				":active": {
					"color": {
						"background": "var(--wp--preset--color--contrast)",
						"text": "var(--wp--preset--color--white)"
					}
				}
			}
		},
		"spacing": {
			"blockGap": "clamp(1.5rem, 1.368rem + 0.66vw, 2rem)"
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--base)"
		}
	}
}
